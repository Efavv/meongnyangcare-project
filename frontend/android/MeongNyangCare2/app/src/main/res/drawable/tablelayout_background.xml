<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 배경색을 연한 초록색으로 설정 -->
    <solid android:color="#F2FFFC"/>

    <!-- 테두리 색상은 기존의 초록색 계열로 설정 -->
    <stroke
        android:color="#099924"
        android:width="2dp"/>

    <!-- 모서리 둥글게 처리 -->
    <corners android:radius="10dp"/> <!-- 모서리 둥글기 강화 -->
</shape>
